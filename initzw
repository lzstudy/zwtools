#!/bin/bash

PATH_BASE=$(cd `dirname $0`; pwd)

########################################################################################################
# 别名初始化
########################################################################################################
function alias_init()
{
    ln -s zwput pt
    ln -s zwget gt
    ln -s zwsample sp
}

########################################################################################################
# 程序入口
########################################################################################################
function initzw_entry()
{
    # 起别名
    alias_init
}

initzw_entry
